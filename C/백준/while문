10952

#include <stdio.h>

int main(void){
    int a,b;
    
    while(1)
    {
        scanf("%d %d",&a,&b);
        if (a==0 && b==0)
            break;
        printf("%d\n",a+b);
            
    }
    return 0;
}

10951

#include <stdio.h>

int main(void) {
    int a,b;
    while(scanf("%d %d", &a, &b)!=EOF) {
        printf("%d\n", a+b);
    }
}


EOF를 입력하란거
