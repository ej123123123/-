#include <stdio.h>

int main(void) {


        int num1, num2, a, b, c;
        scanf("%d %d", &num1, &num2);
        printf("%d\n", num1 * (num2 % 10));
        a = num1 * (num2 % 10);
            num2 = num2 / 10;
        printf("%d\n", num1 * (num2 % 10));
        b = num1 * (num2 % 10);
            num2 = num2 / 10;
        printf("%d\n", num1 * num2);
        c = num1 * num2;
        printf("%d\n", (c* 100) +( b * 10) + a);
        return 0;
    }
    
